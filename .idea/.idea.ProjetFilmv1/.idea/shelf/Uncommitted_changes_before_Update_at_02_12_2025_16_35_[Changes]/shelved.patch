+namespace ProjetFilmv1
+{
+    public class Movie
+    {
+        public string Title { get; set; }         // Nom du film
+        public double Rating { get; set; }        // Note 0.0 - 10.0
+        public string ImageUrl { get; set; }      // URL de l'affiche (ou chemin local)
+        
+        // Propriété utilitaire pour l'affichage des étoiles 
+        public string Stars
+        {
+            get
+            {
+                int full = (int)Math.Round(Rating / 2.0); // transforme une note /10 en 0-5 étoiles approximatives
+                return new string('★', full) + new string('☆', Math.Max(0, 5 - full));
+            }
+        }
+    }
+}
